@model kr.Models.test_instance

@{
    ViewBag.Title = "exportResults";
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <hr />
        <h4>Результат отбора:</h4>
        <div class="col-md-2">
            @Html.Label("Прошедшие отбор")
        </div>
        <div class="col-md-10">
            @Html.TextBox("/")
        </div>
        <hr />
        <hr />
        <h4 />
        <div class="col-md-2">
            @Html.Label("Филиал")
        </div>
        @*@Html.ValidationSummary(true, "", new { @class = "text-danger" })*@
        <div class="form-group">
            <div class="col-md-10">
                @Html.DropDownList("gender", null, htmlAttributes: new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.child.gender, "", new { @class = "text-danger" })
            </div>
        </div>


        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Создать отчет" class="btn btn-default" />
            </div>
        </div>
    </div>
}


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
